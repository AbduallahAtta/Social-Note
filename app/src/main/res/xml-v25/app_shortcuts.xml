<shortcuts xmlns:android="http://schemas.android.com/apk/res/android">
	<shortcut
			android:shortcutId="createNoteShortcut"
			android:enabled="true"
			android:icon="@drawable/ic_create_note_shortcut"
			android:shortcutShortLabel="@string/create_note_shortcut_short_label"
			android:shortcutLongLabel="@string/create_note_shortcut_long_label"
			android:shortcutDisabledMessage="@string/create_note_shortcut_disabled_message">
		<intent
				android:action="android.intent.action.VIEW"
				android:targetPackage="elamien.abdullah.socialnote"
				android:targetClass="elamien.abdullah.socialnote.ui.AddEditNoteActivity"/>
		<categories android:name="android.shortcut.conversation"/>
	</shortcut>

</shortcuts>